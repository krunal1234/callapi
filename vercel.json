{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/tesseract.js": {
      "runtime": "nodejs16.x",
      "memory": 1024
    }
  },
  "static": {
    "directory": "public"
  },
  "routes": [
    {
      "src": "/swagger.json",
      "dest": "/public/swagger.json"
    },
    {
      "src": "/api-docs(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
